{
  "compilerOptions": {
    "lib": ["deno.ns", "deno.window", "dom"],
    "types": ["https://deno.land/std@0.190.0/http/server.ts", "https://esm.sh/@supabase/supabase-js@2.45.0"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler"
  },
  "include": ["./**/*.ts"]
}