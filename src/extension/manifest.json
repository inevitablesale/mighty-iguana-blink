{
  "manifest_version": 3,
  "name": "Coogi Helper Extension",
  "version": "1.0.1",
  "description": "Connects with the Coogi web application to enable advanced features.",
  "content_scripts": [
    {
      "matches": [
        "https://coogi.netlify.app/*"
      ],
      "js": [
        "handshake.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://*.linkedin.com/*"
      ],
      "js": [
        "scraper.js"
      ]
    }
  ]
}